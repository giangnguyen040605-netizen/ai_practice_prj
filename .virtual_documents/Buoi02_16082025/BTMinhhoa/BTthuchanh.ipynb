def Tinh(n):
    tong= 0
    dem = 0
    while(n>0):
        sodu = n%10
        tong = tong+sodu
        dem = dem+1
        n = n//10
        print(sodu)
    return tong,dem
a = int(input("Nhap n:"))
tong, sochuso = Tinh(a)
print("so luong chu so: %d" % sochuso)
print("tong cac chu so:%d" %tong)





def giaithua(n):
    if n == 1:
        return 1
    else:
        return (n * giaithua(n-1))
num = 5
print("Giai thừa của", num, "là", giaithua(num))





def power(a,b):
        if b==0:
            return 1
        elif a==0:
            return 0
        elif b==1:
            return a
        else:
            return a*power(a,b-1)
a = 2
b = 3
print(a,"^",b,"=", power(a,b))








